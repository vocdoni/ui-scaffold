{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "~components/*": ["components/*"],
      "~constants": ["constants/index.ts"],
      "~constants/*": ["constants/*"],
      "~elements/*": ["elements/*"],
      "~i18n": ["i18n/index.ts"],
      "~i18n/*": ["i18n/*"],
      "~routes": ["router/routes/index.ts"],
      "~shared/*": ["components/shared/*"],
      "~theme": ["theme/index.ts"],
      "~theme/*" : ["theme/*"],
      "~queries/*": ["queries/*"],
      "~utils/*": ["utils/*"],
      "~src/*": ["./*"]
    }
  }
}
